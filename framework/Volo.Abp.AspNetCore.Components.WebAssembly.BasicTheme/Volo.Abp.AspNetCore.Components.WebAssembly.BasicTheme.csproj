<Project Sdk="Microsoft.NET.Sdk.Razor">

    <Import Project="..\..\configureawait.props" />
    <Import Project="..\..\common.props" />

    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
		<PackageId>Snow.ABA.AspNetCore.Components.WebAssembly.BasicTheme</PackageId>
		<Authors>Snow</Authors>
		<Product>$(PackageId)</Product>
		<Version>0.1.0</Version>
		<PackageProjectUrl />
		<Description>abp blazorUi antd extension</Description>
		<RepositoryUrl>https://github.com/snowchenlei/abp-blazor-antd</RepositoryUrl>
    </PropertyGroup>

    <ItemGroup>
	    <PackageReference Include="Volo.Abp.Http.Client.IdentityModel.WebAssembly" Version="5.0.0" />
	    <ProjectReference Include="..\Volo.Abp.AspNetCore.Components.Web.BasicTheme\Volo.Abp.AspNetCore.Components.Web.BasicTheme.csproj" />
	    <ProjectReference Include="..\Volo.Abp.AspNetCore.Components.WebAssembly.Theming\Volo.Abp.AspNetCore.Components.WebAssembly.Theming.csproj" />
    </ItemGroup>

    <ItemGroup>
      <PackageReference Update="Fody" Version="6.5.3">
        <PrivateAssets>all</PrivateAssets>
        <IncludeAssets>runtime; build; native; contentfiles; analyzers</IncludeAssets>
      </PackageReference>
    </ItemGroup>

</Project>
